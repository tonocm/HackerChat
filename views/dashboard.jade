extends layout

block content
  title= HackerChat
  h1= HackerChat
  h1 #{pageData.title}
  p #{pageData.userName}
  form(name="myHackathons", action="/myHackathons")
    input(type="submit", value="My Hackathons")
  ul
    each val, index in pageData.body['results']['collection1']
      li= index + ': ' + val.event_name.text
  p #{pageData.body['results']['collection1']}
  p end

